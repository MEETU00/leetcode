def numIdenticalPairs(self, nums: List[int]) -> int:
  count=0
  for m in range(len(nums)):
    for n in range(m+1,len(nums)):
       if nums[m]==nums[n]:
         count+=1
  return count

***********************

def numIdenticalPairs(self, nums: List[int]) -> int:
  c=0
  for i in range(len(nums)):
    for j in range(i+1,len(nums)):
      if nums[i]==nums[j]:
        c+=c
  return c      
