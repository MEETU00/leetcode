class Solution:
    def xorOperation(self, n: int, start: int) -> int:
        ## RC ##
        ## APPROACH : MATH ##
        res = 0
        for i in range(n):
            res ^= start + 2 * i
        return res

class Solution:
    def xorOperation(self, n: int, start: int) -> int:
        arr=[start+2*i for i in range(n)]
        ans=arr[0]
        for i in arr[1:]:
            ans=ans^i
        return ans
